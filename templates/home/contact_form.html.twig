{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}

{% block body %}
    {{ form_start(contact_form, {'attr': {'class': 'contact-form-wizard leave contact-form'}}) }}
    <div class="tab-content">
        <div class="tab-pane" id="step1">
            <div class="col-lg-4 col-md-6 pe-5" data-aos="fade-up" data-aos-delay="300" data-aos-duration="400">
                <div class="our-skills">
                    <div class="progress-bar" data-effect="1" data-value="100" data-skill="Step 2"></div>
                </div>
            </div>
            {{ form_widget(contact_form.ownerStatus,{'attr': {'class': 'form-control'}}) }}
            {{ form_widget(contact_form.sellingReason, {'attr': {'class': 'form-control'}}) }}
            {{ form_widget(contact_form.listed, {'attr': {'class': 'form-control'}}) }}
            <button class="submit-btn sec-btn mt-3 mb-lg-0 mb-5" id="no-conversion" type="submit">Get my cash offer now</button>
        </div>
    </div>
    {{ form_end(contact_form) }}
    {{ include('form/flush_messages.html.twig') }}
{% endblock %}